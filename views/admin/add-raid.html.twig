{% extends 'base.html.twig' %}

{% block content %}
<div class="row">
    <div class="col-md-12">
        <h2>Add Raid</h2>
        <form method="post" class="form-inline" action="/admin/add-raid">
            <div class="mb-3">
                <label for="name" class="form-label">Name</label>
                <input type="text" class="form-control" id="name" name="name" placeholder="Castle Nathria" />
            </div>

            <button type="submit" class="btn btn-primary">Save</button>
        </form>
    </div>
</div>
{% endblock %}